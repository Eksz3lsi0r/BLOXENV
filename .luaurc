{
    "rules": {
        "align_call_arguments": "off",
        "align_function_params": "off",
        "array_element_comma": "always",
        "array_space_inner": "exactly_one",
        "comment_whitespace": "after_always",
        "compound_assignment_spacing": "exactly_one",
        "do_end": "match_block",
        "global_usage": "allow",
        "if_statement": "multi_line",
        "indentation": {
            "indentSize": 4,
            "useTab": false
        },
        "line_length": {
            "length": 100
        },
        "max_line_length": 100,
        "mixed_operators": "always",
        "named_imports_spacing": "exactly_one",
        "newline_statement_block": "allow_single",
        "newline_table_field": "comma_break",
        "parenthesis_padding": "exactly_one",
        "quote_style": "double_quotes",
        "return_statement_spacing": "exactly_one",
        "semicolon": "never",
        "table_separator_style": "comma",
        "trailing_comma": "always",
        "unused_variable": "disabled",
        "white_space": "style_guide"
    }
}