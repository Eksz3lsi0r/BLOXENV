{
    "$schema": "https://raw.githubusercontent.com/rojo-rbx/rojo/master/plugin/assets/aftman-schema.json",
    "name": "BasicGame",
    "tree": {
        "$className": "DataModel",
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "modules": {
                "$path": "src/modules"
            },
            "shared": {
                "$path": "src/shared"
            }
        },
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "server": {
                "$path": "src/server"
            },
            "main": {
                "$path": "src/main.server.lua"
            },
            "EnhancedRunnerSpawner": {
                "$path": "server/EnhancedRunnerSpawner.lua"
            }
        },
        "StarterPlayer": {
            "$className": "StarterPlayer",
            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "client": {
                    "$path": "src/client"
                },
                "main": {
                    "$path": "src/main.client.lua"
                }
            },
            "StarterCharacterScripts": {
                "$className": "StarterCharacterScripts",
                "character": {
                    "$path": "src/character"
                }
            }
        }
    }
}